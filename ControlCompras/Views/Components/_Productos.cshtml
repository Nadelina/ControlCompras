@model IEnumerable<ControlCompras.Models.Producto>

<p>
    @Html.ActionLink("Create New", "Create", "Productoes")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Categoria.Nombre)
